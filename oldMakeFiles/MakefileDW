runTests: DataWranglerTests.class
	java -jar junit5.jar -cp . --select-class=DataWranglerTests

DataWranglerTests.class: DataWranglerTests.java
	javac -cp .:junit5.jar DataWranglerTests.java

DataWranglerTests.java: AirportDatabaseDW.class

AirportDatabaseDW.class: AirportDatabaseDW.java
	javac AirportDatabaseDW.java

AirportDatabaseDW.java:
	javac AirportDW.java
	javac PathDW.java

clean:
	rm *.class
